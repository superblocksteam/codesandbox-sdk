            "type": "string"
          },
          "scopes": {
            "description": "Which scopes to grant this token. The given scopes will replace the current scopes, revoking any that are no longer present in the list.",
            "items": {
              "enum": [
                "sandbox_create",
                "sandbox_edit_code",
                "sandbox_read",
                "vm_manage"
              ]
            },
            "type": "array"
