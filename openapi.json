              "data": {
                "properties": {
                  "description": { "nullable": true, "type": "string" },
                  "expires_at": { "nullable": true, "type": "string" },
                  "scopes": { "items": { "type": "string" }, "type": "array" },
                  "team_id": { "type": "string" },
                  "token": { "type": "string" },
                  "token_id": { "type": "string" }
